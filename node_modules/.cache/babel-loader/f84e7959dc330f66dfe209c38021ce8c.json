AAA,GAAA,MAAA,CAAA,SAAA,EAAA,YAAA,CAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,YAAA,GAAA,MAAA,GAAA,MAAA,CAAA,MAAA,CAAA;AACA,UAAA,IAAA,CAAA,GAAA,CAAA,GAAA,EAAA,OAAA;AACA,cAAA,UAAA,GAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA;AACA,cAAA,WAAA,GAAA,IAAA,CAAA,MAAA,CAAA,IAAA,CAAA;AACA,cAAA,SAAA,GAAA,IAAA,CAAA,MAAA,CAAA,IAAA,CAAA;AACA,cAAA,WAAA,GAAA,IAAA,CAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,KAAA,CAAA,eAAA,GAAA,UAAA,GAAA,WAAA,GAAA,SAAA,GAAA,WAAA;AACA,cAAA,YAAA,GAAA,KAAA,CAAA,eAAA,GAAA,IAAA,CAAA,cAAA;AACA,cAAA,CAAA,GAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,cAAA,EAAA,YAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,UAAA;AACA,SArCA,MAqCA;AACA,UAAA,KAAA,CAAA,KAAA,CAAA;AACA,YAAA,IAAA,EAAA,OADA;AAEA,YAAA,KAAA,EAAA,sBAFA;AAGA,YAAA,IAAA,EAAA,QAAA,CAAA,IAAA,CAAA,YAHA;AAIA,YAAA,iBAAA,EAAA;AAJA,WAAA;;AAMA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AACA,OAnDA,EAoDA,KApDA,CAoDA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,KAAA,EAAA,yBAFA;AAGA,UAAA,IAAA,EAAA,8BAAA,KAHA;AAIA,UAAA,iBAAA,EAAA;AAJA,SAAA;;AAMA,QAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,OA5DA;AA6DA;AApEA,IAhDA;AAsHA,EAAA,OAtHA,qBAsHA,CAAA;AAtHA,CAAA","sourcesContent":["<script>\nimport axios from \"axios\";\n\nimport Layout from \"../../layouts/auth\";\nimport {\n  authMethods,\n  authFackMethods,\n  notificationMethods,\n} from \"@/state/helpers\";\nimport { mapState } from \"vuex\";\n\nimport appConfig from \"@/app.config\";\nimport { required, email } from \"vuelidate/lib/validators\";\n\nexport default {\n  page: {\n    title: \"Login\",\n    meta: [\n      {\n        name: \"description\",\n        content: appConfig.description,\n      },\n    ],\n  },\n  components: {\n    Layout,\n  },\n  data() {\n    return {\n      email: null,\n      password: null,\n      submitted: false,\n      authError: null,\n      tryingToLogIn: false,\n      isAuthError: false,\n      Themesbrand: appConfig.websiteName,\n      logointitle: appConfig.logointitle,\n      titleHeader: appConfig.title,\n      ForgotYourPassword: appConfig.ForgotYourPassword,\n      Signup: appConfig.Signup,\n      CreateWith: appConfig.CreateWith,\n      footerDesign: appConfig.footerDesign,\n      DonAccount: appConfig.DonAccount,\n      Login: appConfig.Login,\n      titleImage: appConfig.title,\n    };\n  },\n  validations: {\n    email: {\n      required,\n      email,\n    },\n    password: {\n      required,\n    },\n  },\n  computed: {\n    ...mapState(\"authfack\", [\"status\"]),\n    notification() {\n      return this.$store ? this.$store.state.notification : null;\n    },\n  },\n  methods: {\n    ...authMethods,\n    ...authFackMethods,\n    ...notificationMethods,\n    tryToLogIn() {\n      this.submitted = true;\n      this.$v.$touch();\n          axios\n          .post(`${process.env.VUE_APP_ENDPOINT}`+'/login', {\n            f_login: this.password,\n          })\n            .then((response) => {\n              if(response.data.messagesboxs == 'Success'){\n              let data = response.data.result[0];\n              this.$store.commit('setLogin',data);\n              this.$store.commit('setRunning',data.f_running);\n              this.$store.commit('setHospital',data.f_hospital);\n              this.$store.commit('setZone',data.f_zone);\n              this.$store.commit('setProvince',data.f_province);\n              const token = response.data.token;\n              const user = response;\n              localStorage.setItem('token', response.data.token);\n              localStorage.setItem('f_positionCode', data.f_positionCode);\n              localStorage.setItem('f_code', data.f_code);\n              localStorage.setItem('f_hospitalCode', data.f_hospitalCode);\n              localStorage.setItem('f_hospitalLevel', data.f_hospitalLevel);\n              localStorage.setItem('f_level', data.f_level);\n              localStorage.setItem('f_level1', data.f_level1);\n              localStorage.setItem('f_login', data.f_login);\n              localStorage.setItem('f_positionCode', data.f_positionCode);\n              localStorage.setItem('f_positionName', data.f_positionName);\n              localStorage.setItem('f_province', data.f_province);\n              localStorage.setItem('f_username', data.f_username);\n              localStorage.setItem('f_userCode', data.f_userCode);\n              localStorage.setItem('f_zone', data.f_zone);\n              const toDayData = new Date();\n              let date = moment(toDayData, \"DD/MM/YYYY\");\n              moment.locale('th');\n              this.dataDateTime = moment().format('LLLL');\n                date.add(543, \"years\");\n                let runnigYear = date.format(\"YYYY\");\n                let runnigMonth = date.format(\"MM\");\n                let runnigDay = date.format(\"DD\");\n                let runningTime = date.format('hhmmss')\n                this.toDayDataFormat = runnigYear + runnigMonth + runnigDay + runningTime;\n                let f_docrunning = this.toDayDataFormat + data.f_hospitalCode;\n                let d = date.format(\"YYYY\");\n                localStorage.setItem('f_docrunning',f_docrunning);\n                this.$router.push('/profile')\n              }else{\n                  this.$swal({\n                    icon: \"error\",\n                    title: \"ไม่พบผู้ใช้งานในระบบ\",\n                    text: response.data.messagesboxs,\n                    allowOutsideClick: false,\n                  });\n                  this.$router.push('/login');\n              }\n            })\n            .catch(error => {\n              this.$swal({\n                    icon: \"error\",\n                    title: \"ไม่สามารถเข้าสู่ระบบได้\",\n                    text: 'กรุณาติดต่อเจ้าหน้าที่ : '+ error,\n                    allowOutsideClick: false,\n                  });\n              this.$router.push('/login');                  \n            })\n    },\n  },\n  mounted() {},\n};\n</script>\n\n<template>\n  <Layout>\n    <div class=\"row justify-content-center\">\n      <div class=\"col-md-8 col-lg-6 col-xl-5\">\n        <div class=\"card overflow-hidden\">\n          <div class=\"bg-soft bg-primary\">\n            <div class=\"row\">\n              <div class=\"col-7\">\n                <div class=\"text-primary p-4\">\n                  <h5 class=\"text-primary\">{{ logointitle }}</h5>\n                  <p>{{ titleHeader }}.</p>\n                </div>\n              </div>\n              <div class=\"col-5 align-self-end\">\n                <img\n                  src=\"@/assets/images/profile-img.png\"\n                  :alt=\"titleImage\"\n                  class=\"img-fluid\"\n                />\n              </div>\n            </div>\n          </div>\n          <div class=\"card-body pt-0\">\n            <div>\n              <router-link tag=\"a\" to=\"/\">\n                <div class=\"avatar-md profile-user-wid mb-4\">\n                  <span class=\"avatar-title rounded-circle bg-light\">\n                    <img\n                      src=\"@/assets/images/rh_anamai.png\"\n                      :alt=\"titleImage\"\n                      height=\"80\"\n                    />\n                  </span>\n                </div>\n              </router-link>\n            </div>\n            <b-alert\n              v-model=\"isAuthError\"\n              variant=\"danger\"\n              class=\"mt-3\"\n              dismissible\n              >{{ authError }}</b-alert\n            >\n            <div\n              v-if=\"notification.message\"\n              :class=\"'alert ' + notification.type\"\n            >\n              {{ notification.message }}\n            </div>\n\n            <b-form class=\"p-2\" @submit.prevent=\"tryToLogIn\">\n              <b-form-group\n                class=\"mb-3\"\n                id=\"input-group-2\"\n                label=\"Password\"\n                label-for=\"input-2\"\n              >\n                <b-form-input\n                  id=\"input-2\"\n                  v-model=\"password\"\n                  type=\"password\"\n                  placeholder=\"Enter password\"\n                  :class=\"{ 'is-invalid': submitted && $v.password.$error }\"\n                ></b-form-input>\n                <div\n                  v-if=\"submitted && !$v.password.required\"\n                  class=\"invalid-feedback\"\n                >\n                  Password is required.\n                </div>\n              </b-form-group>\n              <router-link tag=\"a\" to=\"/forgot-password\" class=\"text-muted\">\n                <i class=\"mdi mdi-lock me-1\"></i> {{ ForgotYourPassword }}\n              </router-link>\n              <div class=\"mt-3 d-grid\">\n                <b-button type=\"submit\" variant=\"primary\" class=\"btn-block\">{{\n                  Login\n                }}</b-button>\n              </div>\n            </b-form>\n          </div>\n        </div>\n\n        <div class=\"mt-5 text-center\">\n          <p>\n            {{ DonAccount }}\n            <router-link\n              tag=\"a\"\n              to=\"/register\"\n              class=\"fw-medium text-primary\"\n              >{{ Signup }}</router-link\n            >\n          </p>\n          <p>\n            © {{ new Date().getFullYear() }} YFHS. {{ CreateWith }}\n            <i class=\"mdi mdi-heart text-danger\"></i> by {{ footerDesign }}\n          </p>\n        </div>\n      </div>\n    </div>\n  </Layout>\n</template>\n\n<style lang=\"scss\" module></style>\n"],"sourceRoot":"src/router/views/account"}]}