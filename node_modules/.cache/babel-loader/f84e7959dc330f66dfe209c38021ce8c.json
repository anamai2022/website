A,MAAA,KAAA,EAAA,SAAA,CAAA,KAfA;AAgBA,MAAA,UAAA,EAAA,SAAA,CAAA;AAhBA,KAAA;AAkBA,GAhCA;AAiCA,EAAA,WAAA,EAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,QAAA,EAAA,QADA;AAEA,MAAA,KAAA,EAAA;AAFA,KADA;AAKA,IAAA,QAAA,EAAA;AACA,MAAA,QAAA,EAAA;AADA;AALA,GAjCA;AA0CA,EAAA,QAAA,kCACA,QAAA,CAAA,UAAA,EAAA,CAAA,QAAA,CAAA,CADA;AAEA,IAAA,YAFA,0BAEA;AACA,aAAA,KAAA,MAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,YAAA,GAAA,IAAA;AACA;AAJA,IA1CA;AAgDA,EAAA,OAAA,8DACA,WADA,GAEA,eAFA,GAGA,mBAHA;AAIA,IAAA,UAJA,wBAIA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,EAAA,CAAA,MAAA;;AAEA,UAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AACA;AACA,OAFA,MAEA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,oBAAA,KAAA,SAAA,EAAA;AACA,UAAA,KAAA,CACA,IADA,CACA,iCADA,EACA;AACA,YAAA,KAAA,EAAA,KAAA,KADA;AAEA,YAAA,QAAA,EAAA,KAAA;AAFA,WADA,EAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,mBAAA,GAAA;AACA,WAPA;AAQA;AACA;AACA;AAtBA,IAhDA;AAwEA,EAAA,OAxEA,qBAwEA,CAAA;AAxEA,CAAA","sourcesContent":["<script>\nimport axios from \"axios\";\n\nimport Layout from \"../../layouts/auth\";\nimport {\n  authMethods,\n  authFackMethods,\n  notificationMethods,\n} from \"@/state/helpers\";\nimport { mapState } from \"vuex\";\n\nimport appConfig from \"@/app.config\";\nimport { required, email } from \"vuelidate/lib/validators\";\n\nexport default {\n  page: {\n    title: \"Login\",\n    meta: [\n      {\n        name: \"description\",\n        content: appConfig.description,\n      },\n    ],\n  },\n  components: {\n    Layout,\n  },\n  data() {\n    return {\n      email: null,\n      password: null,\n      submitted: false,\n      authError: null,\n      tryingToLogIn: false,\n      isAuthError: false,\n      Themesbrand: appConfig.websiteName,\n      logointitle: appConfig.logointitle,\n      titleHeader: appConfig.title,\n      ForgotYourPassword: appConfig.ForgotYourPassword,\n      Signup: appConfig.Signup,\n      CreateWith: appConfig.CreateWith,\n      footerDesign: appConfig.footerDesign,\n      DonAccount: appConfig.DonAccount,\n      Login: appConfig.Login,\n      titleImage: appConfig.title,\n    };\n  },\n  validations: {\n    email: {\n      required,\n      email,\n    },\n    password: {\n      required,\n    },\n  },\n  computed: {\n    ...mapState(\"authfack\", [\"status\"]),\n    notification() {\n      return this.$store ? this.$store.state.notification : null;\n    },\n  },\n  methods: {\n    ...authMethods,\n    ...authFackMethods,\n    ...notificationMethods,\n    tryToLogIn() {\n      this.submitted = true;\n      this.$v.$touch();\n\n      if (this.$v.$invalid) {\n        return;\n      } else {\n         if (process.env.VUE_APP_DEFAULT_AUTH === \"authapi\") {\n          axios\n            .post(\"http://127.0.0.1:8000/api/login\", {\n              email: this.email,\n              password: this.password,\n            })\n            .then((res) => {\n              return res;\n            });\n        }\n      }\n    },\n  },\n  mounted() {},\n};\n</script>\n\n<template>\n  <Layout>\n    <div class=\"row justify-content-center\">\n      <div class=\"col-md-8 col-lg-6 col-xl-5\">\n        <div class=\"card overflow-hidden\">\n          <div class=\"bg-soft bg-primary\">\n            <div class=\"row\">\n              <div class=\"col-7\">\n                <div class=\"text-primary p-4\">\n                  <h5 class=\"text-primary\">{{logointitle}}</h5>\n                  <p>{{titleHeader}}.</p>\n                </div>\n              </div>\n              <div class=\"col-5 align-self-end\">\n                <img\n                  src=\"@/assets/images/profile-img.png\"\n                  :alt=\"titleImage\"\n                  class=\"img-fluid\"\n                />\n              </div>\n            </div>\n          </div>\n          <div class=\"card-body pt-0\">\n            <div>\n              <router-link tag=\"a\" to=\"/\">\n                <div class=\"avatar-md profile-user-wid mb-4\">\n                  <span class=\"avatar-title rounded-circle bg-light\">\n                    <img src=\"@/assets/images/rh_anamai.png\" :alt=\"titleImage\" height=\"80\" />\n                  </span>\n                </div>\n              </router-link>\n            </div>\n            <b-alert\n              v-model=\"isAuthError\"\n              variant=\"danger\"\n              class=\"mt-3\"\n              dismissible\n              >{{ authError }}</b-alert\n            >\n            <div\n              v-if=\"notification.message\"\n              :class=\"'alert ' + notification.type\"\n            >\n              {{ notification.message }}\n            </div>\n\n            <b-form class=\"p-2\" @submit.prevent=\"tryToLogIn\">\n                <b-form-group\n                class=\"mb-3\"\n                id=\"input-group-2\"\n                label=\"Password\"\n                label-for=\"input-2\"\n              >\n                <b-form-input\n                  id=\"input-2\"\n                  v-model=\"password\"\n                  type=\"password\"\n                  placeholder=\"Enter password\"\n                  :class=\"{ 'is-invalid': submitted && $v.password.$error }\"\n                ></b-form-input>\n                <div\n                  v-if=\"submitted && !$v.password.required\"\n                  class=\"invalid-feedback\"\n                >\n                  Password is required.\n                </div>\n              </b-form-group>\n                <router-link tag=\"a\" to=\"/forgot-password\" class=\"text-muted\">\n                  <i class=\"mdi mdi-lock me-1\"></i> {{ForgotYourPassword}}\n                </router-link>\n              <div class=\"mt-3 d-grid\">\n                <b-button type=\"submit\" variant=\"primary\" class=\"btn-block\">{{Login}}</b-button>\n              </div>\n            </b-form>\n          </div>\n        </div>\n\n        <div class=\"mt-5 text-center\">\n          <p>\n            {{DonAccount}}\n            <router-link\n              tag=\"a\"\n              to=\"/register\"\n              class=\"fw-medium text-primary\"\n              >{{Signup}}</router-link\n            >\n          </p>\n          <p>\n            © {{ new Date().getFullYear() }} YFHS. {{CreateWith}}\n            <i class=\"mdi mdi-heart text-danger\"></i> by {{footerDesign}}\n          </p>\n        </div>\n      </div>\n    </div>\n  </Layout>\n</template>\n\n<style lang=\"scss\" module></style>\n"],"sourceRoot":"src/router/views/account"}]}